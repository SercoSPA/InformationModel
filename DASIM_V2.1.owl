<http://www.semanticweb.org/utente/ontologies/2021/9/DASIM2.1> rdf:type owl:Ontology ;
                                                                rdfs:comment "This is the Ontology (V2.1) that describes the Data Access Services Information Model"@en .

#################################################################
#    Datatypes
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2021/9/Sysma1.0#interval
:interval rdf:type rdfs:Datatype ;
          rdfs:comment "interval of integers" ;
          owl:equivalentClass [ rdf:type rdfs:Datatype ;
                                owl:onDatatype xsd:integer ;
                                owl:withRestrictions ( [ xsd:minInclusive 0
                                                       ]
                                                       [ xsd:maxInclusive 10000
                                                       ]
                                                     )
                              ] .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/utente/ontologies/2021/9/Sysma1.0#archiving
:archiving rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :storing ;
           rdfs:domain [ rdf:type owl:Restriction ;
                         owl:onProperty :archiving ;
                         owl:someValuesFrom :product_event
                       ] ;
           rdfs:range [ rdf:type owl:Restriction ;
                        owl:onProperty :archiving ;
                        owl:someValuesFrom :product
                      ] ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :archiving ;
                        owl:someValuesFrom :service_component
                      ] ;
           rdfs:comment "The Archiving view is the result of a select query on the Product_Storing view, where service_component=”archiver”."@en .


###  http://www.semanticweb.org/utente/ontologies/2021/9/Sysma1.0#deletion
:deletion rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Restriction ;
                        owl:onProperty :deletion ;
                        owl:someValuesFrom :product_event
                      ] ;
          rdfs:range [ rdf:type owl:Restriction ;
                       owl:onProperty :deletion ;
                       owl:someValuesFrom :product
                     ] ,
                     [ rdf:type owl:Restriction ;
                       owl:onProperty :deletion ;
                       owl:someValuesFrom :service_component
                     ] ;
          rdfs:comment "Only the Product_event table’s rows with event_type=”deletion” are displayed in this view."@en .


